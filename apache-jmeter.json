{
    "homepage": "http://jmeter.apache.org/",
    "version": "3.3",
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=/jmeter/binaries/apache-jmeter-3.3.zip",
            "hash": "sha512:23c72d38f2ab45b4d21349610578aa84829f58cbbc9239196b5c2d7168460e88d863add8708e9c3048841fe4fbc1c410213bfe7a0df04368910ffeab99ba1f02"
        }
    },
    "extract_dir": "apache-jmeter-3.3",
    "bin": "bin\\jmeter.bat",
    "env_set": {
        "JMETER_HOME": "$dir"
    },
    "suggest": {
        "JRE": [
            "extras/oraclejre-server",
            "openjdk"
        ]
    },
    "checkver": {
        "url": "http://jmeter.apache.org/changes.html",
        "re": "v(?<Version> [\\d]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=/jmeter/binaries/apache-jmeter-$version.zip",
                "hash": {
                    "url": "https://www.apache.org/dist/jmeter/binaries/apache-jmeter-$version.zip.sha512",
                    "find": "([a-fA-F0-9]+)"
                }
            }
        },
        "extract_dir": "apache-jmeter-$version"
    }
}
