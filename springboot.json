{
    "homepage": "http://projects.spring.io/spring-boot/",
    "version": "1.5.6",
    "license": "Apache License 2.0",
    "url": "http://repo.spring.io/release/org/springframework/boot/spring-boot-cli/1.5.6.RELEASE/spring-boot-cli-1.5.6.RELEASE-bin.zip",
    "hash": "sha1:bd88bc705b136c309c89d6812629fbbadce11635",
    "extract_dir": "spring-1.5.6.RELEASE",
    "bin": [
        "bin\\spring.bat"
    ],
    "env_set": {
        "SPRING_HOME": "$dir"
    },
    "suggest": {
        "JDK": [
            "extras/oraclejdk",
            "openjdk"
        ]
    },
    "checkver": {
        "url": "http://repo.spring.io/release/org/springframework/boot/spring-boot-cli/",
        "re": "scala-([\\d.]+).RELEASE"
    },
    "autoupdate": {
        "url": "http://repo.spring.io/release/org/springframework/boot/spring-boot-cli/$version.RELEASE/spring-boot-cli-$version.RELEASE-bin.zip",
        "extract_dir": "spring-$version.RELEASE"
    }
}
